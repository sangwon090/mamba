let a: i32 = 2;
let b: i32 = 5;
let c: i32 = -(3 - (a * b));
let d: str = "It works on my machine :)";

extern puts(text: str) -> i32;
extern fflush(buffer: i32) -> i32;
extern clock() -> i32;

def fibo(n: i32) -> i32:
    if n <= 0:
        return 0;
    else:
        if n == 1:
            return 1;
        else:
            return fibo(n - 2) + fibo(n - 1);

def main() -> i32:
    puts("Hello, world!");
    puts(d);
    fflush(0);
    return clock();