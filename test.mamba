let a: int = 2;
let b: int = 5;
let c: int = -(3 - (a * b));
let d: str = "It works on my machine :)";

extern puts(text: str) -> int;
extern fflush(buffer: int) -> int;
extern clock() -> int;

def fibo(n: int) -> int:
    if n <= 0:
        return 0;
    else:
        if n == 1:
            return 1;
        else:
            return fibo(n - 2) + fibo(n - 1);

